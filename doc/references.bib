@article{pinkeletal91,
  title = {Strain: {{Observations}} of the {{Vertical Gradient}} of {{Isopycnal Vertical Displacement}}},
  shorttitle = {Strain},
  author = {Pinkel, Robert and Sherman, Jeffrey and Smith, Jerome and Anderson, Steven},
  year = {1991},
