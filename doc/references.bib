@article{heuzeetal15,
  title = {Changes in Global Ocean Bottom Properties and Volume Transports in {{CMIP5}} Models under Climate Change Scenarios},
  author = {Heuz{\'e}, C{\'e}line and Heywood, Karen J. and Stevens, David P. and Ridley, Jeff K.},
  year = {2015},
