@article{zhengetal22,
  title = {Fine-{{Scale Velocity Measurement}} on the {{Wirewalker Wave-Powered Profiler}}},
  author = {Zheng, Bofu and Lucas, Andrew J. and Pinkel, Robert and Le Boyer, Arnaud},
