@article{shaoetal18,
  title = {Turbulent Mixing on Sloping Bottom of an Energetic Tidal Channel},
  author = {Shao, Huan-Jie and Tseng, Ruo-Shan and Lien, Ren-Chieh and Chang, Yu-Chia and Chen, Jau-Ming},
  year = {2018},
  month = aug,
  journal = {Cont. Shelf Res.},
  volume = {166},
