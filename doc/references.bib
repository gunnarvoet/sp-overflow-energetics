@article{mcdonaghetal02,
  title = {On the Circulation of Bottom Water in the Region of the {{Vema Channel}}},
  author = {McDonagh, Elaine L. and Arhan, Michel and Heywood, Karen J.},
  year = {2002},
  month = jul,
  journal = {Deep Sea Research Part I: Oceanographic Research Papers},
  volume = {49},
  number = {7},
  pages = {1119--1139},
  issn = {0967-0637},
  doi = {10.1016/S0967-0637(02)00016-X},
  abstract = {The circulation and transport of Antarctic Bottom Water ({$\sigma$}4{$<$}45.87) in the region of the Vema Channel are studied along three WOCE hydrographic lines, the geostrophic velocities referenced to previously published direct current measurements. The primary supply of water to the deep Vema Channel is from the Argentine Basin's deep western boundary current, with no indication of an inflow from the southeast. In the northern Argentine Basin, detachment of lower North Atlantic Deep Water from the continental slope is associated with a deep thermohaline front near 34\textdegree S. To the north of this front, the upper part of the AABW bound for the Vema Channel ({$\sigma$}4{$<$}46.01) exhibits a significant NADW influence. Further modification of the throughflow water occurs near 30\textdegree 30{${'}$}S, where the channel orientation changes by {$\sim$}50\textdegree. Southward flow of bottom water on the eastern flank of the Vema Channel, amounting to {$\sim$}1.5Sv, represents a significant countercurrent to the deep channel transport. Inclusion of this countercurrent reduces the net flow of AABW through the Vema Channel from 3.2{$\pm$}0.7 to 1.7{$\pm$}1.1Sv. Water properties imply that the near-zero net flow over the Santos Plateau results from a near-closed cyclonic circulation fed by the deep Vema Channel throughflow. A disruption of the northward boundary current in the upper AABW (lower circumpolar water) is required by this flow pattern. The extension of the cyclonic circulation on the Santos Plateau enters the Brazil Basin as a {$\sim$}1Sv flow distinct from the outflow in the Vema Channel Extension (6.2Sv). The high magnitude of the latter suggests a southward recirculation of bottom water near the western boundary to the north of the region of study.},
  langid = {english},
  keywords = {Abyssal circulation,Antarctic bottom water,Argentine Basin,Brazil Basin,Deep front,North Atlantic deep water,South Atlantic,Vema Channel},
  file = {/Users/gunnar/Projects/gvzbib/articles/McDonagh/mcdonagh 2002 on the circulation of bottom water in the region of the vema channel.pdf}
}

@article{mcdonaghetal08,
  title = {The Circulation of the {{Indian Ocean}} at 32\textdegree{{S}}},
  author = {McDonagh, Elaine L. and Bryden, Harry L. and King, Brian A. and Sanders, Richard J.},
