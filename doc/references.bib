@article{wolfecessi11,
  title = {The {{Adiabatic Pole-to-Pole Overturning Circulation}}},
  author = {Wolfe, Christopher L. and Cessi, Paola},
  year = {2011},
  month = sep,
  journal = {Journal of Physical Oceanography},
