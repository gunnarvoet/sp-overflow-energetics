@article{carteretal06,
  title = {Flow and {{Mixing}} around a {{Small Seamount}} on {{Kaena Ridge}}, {{Hawaii}}},
  author = {Carter, Glenn S. and Gregg, Michael C. and Merrifield, Mark A.},
