@article{thompsonetal07,
  title = {Spatial and {{Temporal Patterns}} of {{Small-Scale Mixing}} in {{Drake Passage}}},
  author = {Thompson, Andrew F. and Gille, Sarah T. and MacKinnon, J. A. and Sprintall, Janet},
  year = {2007},
