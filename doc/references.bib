@article{laneserffwoodward01,
  title = {Internal Bores in Two-Layer Exchange \#ows over Sills},
  author = {{Lane-Serff}, G F and Woodward, M D},
  year = {2001},
  abstract = {Internal bores are a common feature of tidally modulated two-layer exchange \#ows through straits and over sills. Even where the forcing changes smoothly, the \#ow may adjust with sudden jumps in the position of the interface between the two layers. The resulting \#ow con"guration, with a hydraulically controlled exchange \#ow (at the sill) coupled with a propagating internal hydraulic jump (known as a bore), is investigated with mathematical models and laboratory experiments. The study concentrates on twodimensional \#ow in a rectangular channel with a sill. The parameters considered are the depth of the channel compared to the depth over the sill, the depth of the interface before the passage of the bore and the strength of the net \#ux through the channel.The theory is based on shallow water equations and hydraulic control theory and includes the e!ects of a steady net \#ow through the channel (driven, for example, by the tide). Once the depth of the channel is twice the depth over the sill, further changes in geometry have relatively little e!ect on the \#ow. The bore velocity and \#uxes are strongly a!ected by the strength of any net \#ow. The laboratory experiments model pure exchange \#ows (with no net \#ow) and give detailed information about the bores themselves. In many cases an undular bore is produced, with a well-de"ned wave train on the interface behind the front of the bore. The wavelengths and amplitudes of these internal waves are quanti"ed and a brief comparison with similar internal waves observed in the Strait of Gibraltar is presented. 2000 Elsevier Science Ltd. All rights reserved.},
  langid = {english},
  file = {/Users/gunnar/Projects/gvzbib/articles/Lane-Serff/lane-serff 2001 internal bores in two-layer exchange #ows over sills.pdf}
}

@article{lappeumlauf16,
  title = {Efficient Boundary Mixing Due to Near-Inertial Waves in a Non-Tidal Basin: {{Observations}} from the {{Baltic Sea}}},
  shorttitle = {Efficient Boundary Mixing Due to Near-Inertial Waves in a Non-Tidal Basin},
  author = {Lappe, C. and Umlauf, L.},
  year = {2016},
  month = nov,
  journal = {J. Geophys. Res. Oceans},
  issn = {21699275},
  doi = {10.1002/2016JC011985},
  abstract = {Diapycnal mixing in large nontidal basins is often assumed to be related to the effect of near-inertial waves. While the role of near-inertial shear for the generation of shear instabilities in the stratified interior is relatively well understood, much less is known about the impact of near-inertial motions on boundary mixing processes in nontidal systems, mainly owing to the lack of appropriate observations. Here, an extensive data set is discussed, describing the variability of boundary mixing induced by near-inertial motions near the sloping topography of one of the main basins of the Baltic Sea. These data reveal the existence of a vigorously turbulent bottom boundary layer of a few meters thickness covering the entire slope region above and below the permanent halocline that constitutes the main obstacle for vertical transport in the Baltic Sea. Near-bottom turbulence was driven by the nearinertial shear in the frictional boundary layer rather than by breaking of near-inertial waves near critical slopes. Large regions of the bottom boundary layer remained strongly stratified, and therefore, different from the traditional view of inefficient boundary mixing, mixing efficiencies reached values typical for the ocean's interior.},
  langid = {english},
  file = {/Users/gunnar/Projects/gvzbib/articles/Lappe/lappe 2016 efficient boundary mixing due to near-inertial waves in a non-tidal basin.pdf}
}

