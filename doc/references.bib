@article{snowetal16,
  title = {Sensitivity of {{Antarctic Bottom Water}} to {{Changes}} in {{Surface Buoyancy Fluxes}}},
  author = {Snow, Kate and Hogg, Andrew McC and Sloyan, Bernadette M. and Downes, Stephanie M.},
  year = {2016},
