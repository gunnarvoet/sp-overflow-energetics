@article{fer06,
  title = {Scaling Turbulent Dissipation in an {{Arctic}} Fjord},
  author = {Fer, Ilker},
  year = {2006},
  month = jan,
  journal = {Deep Sea Research Part II: Topical Studies in Oceanography},
  volume = {53},
  number = {1-2},
