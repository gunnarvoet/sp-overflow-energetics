@article{dareliusetal11,
  title = {Faroe {{Bank Channel Overflow}}: {{Mesoscale Variability}}*},
  shorttitle = {Faroe {{Bank Channel Overflow}}},
  author = {Darelius, Elin and Fer, Ilker and Quadfasel, Detlef},
  year = {2011},
