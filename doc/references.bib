@article{chengetal17,
  title = {Improved Estimates of Ocean Heat Content from 1960 to 2015},
  author = {Cheng, Lijing and Trenberth, Kevin E. and Fasullo, John and Boyer, Tim and Abraham, John and Zhu, Jiang},
