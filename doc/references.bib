@article{shermanpinkel91,
  title = {Estimates of the {{Vertical Wavenumber}}\textendash{{Frequency Spectra}} of {{Vertical Shear}} and {{Strain}}},
  author = {Sherman, Jeffrey T. and Pinkel, Robert},
  year = {1991},
  month = feb,
  journal = {Journal of Physical Oceanography},
  volume = {21},
  number = {2},
  pages = {292--303},
  publisher = {{American Meteorological Society}},
  issn = {0022-3670, 1520-0485},
  doi = {10.1175/1520-0485(1991)021<0292:EOTVWS>2.0.CO;2},
  abstract = {Abstract Measurements of vertical shear and strain were acquired from the research platform FLIP during the PATCHEX experiment in October, 1986 (34\textdegree N, 127\textdegree W). Vertical sheer was shear from two separate Doppler sonar systems. A long-range sonar, with independent estimates every 18 m, sampled from 150\textendash 1200 m in depth. A short-range sonar measured fine-scale shear over 150\textendash 180 m depth, with 1.5 m vertical resolution. Vertical strain, {$\partial\eta$}/{$\partial$}z, was estimated from two repeatedly profiling CTDs. These sampled to 560 m once every three minutes. The time variation of the strain field is monitored in both Eulerian (fixed-depth) and semi-Lagrangian (isopycnal-following) reference frames, from 150\textendash 406 m depth. Eulerian vertical wavenumber-frequency (m, {$\omega$}) spectra of vertical shear and strain exhibit a frequency dependency which is a strong function of wavenumber ({$\omega-$}2\textendash{$\omega$}0 for m = 0.01\textendash 0.3 cpm). In contrast the semi-Lagrangian strain spectrum is more nearly separable in frequency and wavenumber, in closer agreement with the Garrett\textendash Munk (GM) internal wave spectral model. When a simulated GM shear field is vertically advected by a GM isopycnal displacement field, the resultant Eulerian vertical wavenumber\textendash frequency spectrum exhibits the same qualitative, nonseparable, form as the PATCHEX shear spectrum: The dominant near-inertial waves are Doppler-shifted across all frequency bands, resulting in a ``while'' frequency spectrum at high wavenumbers. Measured ratios of Eulerian shear/strain variance support this interpretation. Higher shear-low strain variances (characteristic of near-inertial waves) are seen at high wavenumber, high encounter frequencies. The conclusion is that internal wave vertical self-advection strongly alters the observed frequency at high vertical wavenumbers in an Eulerian reference frame.},
  chapter = {Journal of Physical Oceanography},
  langid = {english},
  file = {/Users/gunnar/Projects/gvzbib/articles/Sherman/sherman 1991 estimates of the vertical wavenumber–frequency spectra of vertical shear and.pdf}
}

@article{siedlerpaul91,
  title = {Barotropic and Baroclinic Tidal Currents in the Eastern Basins of the {{North Atlantic}}},
  author = {Siedler, Gerold and Paul, Uwe},
  year = {1991},
  journal = {Journal of Geophysical Research: Oceans},
  volume = {96},
  number = {C12},
  pages = {22259--22271},
  issn = {2156-2202},
  doi = {10.1029/91JC02319},
  abstract = {Data from a large-scale moored array in the Iberian and Canary basins are used to determine the energies of barotropic and baroclinic M2 and S2 tides. An analysis of time-varying dynamical modes is performed. The results for barotropic modes confirm the global surface tide model results of Schwiderski (1980) for this region. The barotropic modes dominate in the deep basins, but increased baroclinic contributions are usually found over rough topography. At three locations near the continental slope in the southern Canary Basin the baroclinic modes dominate the barotropic mode. Results from an array of three moorings at the northern part of the Cape Verde Rise show an inverse behavior of barotropic and baroclinic energies, such that the baroclinic energy is steadily enhanced while the barotropic energy is reduced towards the continental margin. The increase in baroclinic energy is consistent with a generation of internal tides close to the shelf by surface tidal forcing over topography. Further evidence for this process is provided by the 2-week periodicity of the first-order baroclinic mode at the slope, corresponding to the spring-neap cycle of the barotropic tide.},
  langid = {english},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/91JC02319},
  file = {/Users/gunnar/Projects/gvzbib/articles/Siedler/siedler 1991 barotropic and baroclinic tidal currents in the eastern basins of the north.pdf}
}

