@article{liebermanetal20,
  title = {Running in {{Tarahumara}} ({{Rar\'amuri}}) {{Culture}}: {{Persistence Hunting}}, {{Footracing}}, {{Dancing}}, {{Work}}, and the {{Fallacy}} of the {{Athletic Savage}}},
