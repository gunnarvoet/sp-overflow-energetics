@article{dushawetal17,
  title = {Acoustic {{Tomography}} in the {{Canary Basin}}: {{Meddies}} and {{Tides}}},
  shorttitle = {Acoustic {{Tomography}} in the {{Canary Basin}}},
  author = {Dushaw, Brian D. and Gaillard, Fabienne and Terre, Thierry},
  year = {2017},
  journal = {Journal of Geophysical Research: Oceans},
  volume = {122},
  number = {11},
  pages = {8983--9003},
  issn = {2169-9291},
  doi = {10.1002/2017JC013356},
  abstract = {An acoustic propagation experiment over 308 km range conducted in the Canary Basin in 1997\textendash 1998 was used to assess the ability of ocean acoustic tomography to measure the flux of Mediterranean water and Meddies. Instruments on a mooring adjacent to the acoustic path measured the southwestward passage of a strong Meddy in temperature, salinity, and current. Over 9 months of transmissions, the acoustic arrival pattern was an initial broad stochastic pulse varying in duration by 250\textendash 500 ms, followed eight stable, identified-ray arrivals. Small-scale sound speed fluctuations from Mediterranean water parcels littered around the sound channel axis caused acoustic scattering. Internal waves contributed more modest acoustic scattering. Based on simulations, the main effect of a Meddy passing across the acoustic path is the formation of many early-arriving, near-axis rays, but these rays are thoroughly scattered by the small-scale Mediterranean-water fluctuations. A Meddy decreases the deep-turning ray travel times by 10\textendash 30 ms. The dominant acoustic signature of a Meddy is therefore the expansion of the width of the initial stochastic pulse. While this signature appears inseparable from the other effects of Mediterranean water in this region, the acoustic time series indicates the steady passage of Mediterranean water across the acoustic path. Tidal variations caused by the mode-1 internal tides were measured by the acoustic travel times. The observed internal tides were partly predicted using a recent global model for such tides derived from satellite altimetry.},
  langid = {english},
  keywords = {acoustic tomography,Canary Basin,internal tides,Meddy,Mediterranean water},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/2017JC013356},
  file = {/Users/gunnar/Projects/gvzbib/articles/Dushaw/dushaw 2017 acoustic tomography in the canary basin.pdf;/Users/gunnar/Zotero/storage/W9FGQQ5Z/2017JC013356.html}
}

