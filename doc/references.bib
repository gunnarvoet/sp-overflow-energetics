@article{ferrariwunsch09,
  title = {Ocean {{Circulation Kinetic Energy}}: {{Reservoirs}}, {{Sources}}, and {{Sinks}}},
  shorttitle = {Ocean {{Circulation Kinetic Energy}}},
  author = {Ferrari, Raffaele and Wunsch, Carl},
  year = {2009},
  month = jan,
  journal = {Annu. Rev. Fluid Mech.},
